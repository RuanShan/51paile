/* auction list */
.nextdays-con{width:1190px;margin:40px auto 0}
.full-con{width:1190px;margin:0 auto}
.nextdays-con .gov-will-title{margin-bottom:50px}
.nextdays-main{position:relative}
.nextdays-bar{position:absolute;top:0;left:-20px;width:3px;height:100%;background:#dedede}
.nextdays-con .nextdays-item{position:relative;margin-bottom:75px;padding-top:10px}
.nextdays-con .three-days-later{margin-bottom:0}
.nextdays-con .three-days-later .pai-con .pai-item{margin-bottom:0}
.nextdays-con .nextdays-item h3{position:absolute;top:-35px;left:0;height:40px;line-height:40px;width:100%;background:#e8e8e8}
.nextdays-con .nextdays-item .left-stamp{position:absolute;bottom:0;left:-68px;width:68px;height:36px}
.nextdays-con .nextdays-item .left-stamp-text{float:left;width:34px;height:33px;text-align:center;color:#fff;background:#19aa6b;font-size:12px;line-height:15px;padding:3px 1px 0}
.nextdays-con .nextdays-item .left-stamp-quan{float:left;width:27px;height:36px;line-height:36px;text-align:center;font-size:12px;color:#c7c7c7}
.nextdays-con .nextdays-item .left-stamp-quan:not(:root){width:28px}
.nextdays-con .nextdays-item .left-stamp-arrow{display:block;border-width:6px;border-style:dashed solid dashed dashed;border-color:transparent #e8e8e8 transparent transparent;font-size:0;overflow:hidden;position:absolute;top:50%;right:0;margin-top:-6px;width:0;height:0;line-height:0}
.nextdays-con .tomorrow .left-stamp-text{color:#fff;background:#19aa6b}
.nextdays-con .tomorrow .left-stamp-quan{color:#19aa6b}
.nextdays-con .tomorrow .left-stamp-arrow{border-color:transparent #19aa6b transparent transparent}
.nextdays-con .nextdays-item h3 a{color:#333;text-decoration:none}
.nextdays-con .nextdays-item h3 .title{padding-left:20px;font-size:20px}
.nextdays-con .nextdays-item h3 .sub-title{padding-left:8px;font-size:16px}
.nextdays-con .nextdays-item h3 .more-count{float:right;font-size:12px;height:40px;line-height:40px;font-weight:400;margin-right:12px;display:inline;*margin-top:-40px}
.nextdays-con .nextdays-item h3 .more-count em{color:#19aa6b;font-size:16px}
.nextdays-con .tomorrow h3{background:#19aa6b}
.nextdays-con .tomorrow h3 a{color:#fff}
.nextdays-con .tomorrow h3 .more-count a{color:#63f2b4}
.nextdays-con .tomorrow h3 .more-count em{color:#fff}


.sf-wrap{margin-top:30px}
.sf-item-list{width:1220px;margin:20px 0 0;overflow:visible;min-height:320px;_height:320px}
.sf-item-list-narrow{width:930px;margin:20px 0 0;overflow:visible;min-height:320px;_height:320px}.sf-pai-item-list{zoom:1;clear:none!important}
.sf-pai-item-list:after{content:"";clear:both;display:block!important;height:0}
.sf-pai-item-list .pai-item{float:left;margin:0 23px 20px 0;_display:inline}
.sf-pai-item-list .pai-item .link-wrap{border:1px solid #eaeaea;display:block;position:relative;width:278px;height:315px}
.sf-pai-item-list .pai-item .link-wrap:hover{margin:-2px;border:3px solid #dc2625;text-decoration:none}
.sf-pai-item-list .pai-item .link-wrap:hover .header-section .title{color:#d91615}
.sf-pai-item-list .pai-item .link-wrap .header-section{display:block;text-align:center;overflow:hidden;position:relative;width:278px;height:185px;line-height:180px}
.sf-pai-item-list .pai-item .link-wrap .header-section .pic{display:inline;vertical-align:middle;max-width:278px}
.sf-pai-item-list .pai-item .link-wrap .header-section .title{white-space:nowrap;text-overflow:ellipsis;overflow:hidden;background-color:#fff;background:rgba(255,255,255,.9);filter:alpha(opacity=90);color:#000;font-size:14px;position:absolute;padding:0 13px;left:0;bottom:0;height:24px;line-height:24px;width:252px;text-align:left}
.sf-pai-item-list .pai-item .link-wrap .info-section{color:#999;display:block;font-size:12px;padding-left:13px;height:131px}
.sf-pai-item-list .pai-item .link-wrap .info-section .price{padding-top:10px;height:30px;line-height:30px}
.sf-pai-item-list .pai-item .link-wrap .info-section .price .value{color:#777;padding-left:10px;font-weight:700}
.sf-pai-item-list .pai-item .link-wrap .info-section .price .value em{font-family:tahoma,'Hiragino Sans GB','\5b8b\4f53',sans-serif;font-size:26px}
.sf-pai-item-list .pai-item .link-wrap .info-section .price .value em s{font-family:'Microsoft YaHei',arial tahoma,'Hiragino Sans GB','\5b8b\4f53',sans-serif;padding-left:4px;text-decoration:none}
.sf-pai-item-list .pai-item .link-wrap .info-section .price .value .currency{font-family:arial,tahoma,'Hiragino Sans GB','\5b8b\4f53',sans-serif;font-weight:400;font-size:16px;padding-right:4px}
.sf-pai-item-list .pai-item .link-wrap .info-section .price .value .price-font-small{font-size:16px}
.sf-pai-item-list .pai-item .link-wrap .info-section .price-todo .value{color:#19aa6b}
.sf-pai-item-list .pai-item .link-wrap .info-section .price-assess{padding-top:2px;line-height:20px;height:20px}
.sf-pai-item-list .pai-item .link-wrap .info-section .price-assess .value .currency,.sf-pai-item-list .pai-item .link-wrap .info-section .price-assess .value em{font-size:12px;font-weight:400}
.sf-pai-item-list .pai-item .link-wrap .info-section .time{padding-top:2px;line-height:20px;height:20px}
.sf-pai-item-list .pai-item .link-wrap .info-section .time .hide-day .pai-day{display:none}
.sf-pai-item-list .pai-item .link-wrap .info-section .time .hide-hour .pai-hour{display:none}
.sf-pai-item-list .pai-item .link-wrap .info-section .time .value{font-family:tahoma,'Hiragino Sans GB','\5b8b\4f53',sans-serif;color:#000;padding-left:10px;font-weight:700}
.sf-pai-item-list .pai-item .link-wrap .footer-section{zoom:1;clear:none!important;border-top:1px solid #eee;position:absolute;left:0;bottom:0;width:278px;height:36px;line-height:36px}
.sf-pai-item-list .pai-item .link-wrap .footer-section:after{content:"";clear:both;display:block!important;height:0}
.sf-pai-item-list .pai-item .link-wrap .footer-section .num-apply,.sf-pai-item-list .pai-item .link-wrap .footer-section .num-auction{color:#999;float:left;padding-left:12px;width:126px}
.sf-pai-item-list .pai-item .link-wrap .footer-section .num-apply em,.sf-pai-item-list .pai-item .link-wrap .footer-section .num-auction em{color:#666;padding-right:4px;font-weight:700}
.sf-pai-item-list .pai-item .link-wrap .footer-section .num-auction{border-right:1px solid #eee}
.sf-pai-item-list .pai-item .link-wrap .flag-section .flag{position:absolute}
.sf-pai-item-list .pai-item .link-wrap .flag-section .flag-doing,.sf-pai-item-list .pai-item .link-wrap .flag-section .flag-todo{background-image:url(<%=image_url "auction_status.png"%>);background-repeat:no-repeat;background-position:0 0;width:70px;height:23px;right:-4px;bottom:45px}
.sf-pai-item-list .pai-item .link-wrap .flag-section .flag-todo{background-image:url(<%=image_url "auction_status_todo.png"%>);background-position:0 0}
.sf-pai-item-list .pai-item .link-wrap .flag-section .flag-break,.sf-pai-item-list .pai-item .link-wrap .flag-section .flag-done,.sf-pai-item-list .pai-item .link-wrap .flag-section .flag-revocation{right:6px;top:85px;width:121px;height:67px}
.sf-pai-item-list .pai-item .link-wrap .flag-section .flag-done{background:url(<%=image_url "auction_status_done.png"%>) no-repeat 0 0}
.sf-pai-item-list .pai-item .link-wrap .flag-section .flag-break{background:url(<%=image_url "auction_status_break.png"%>) no-repeat 0 0}
.sf-pai-item-list .pai-item .link-wrap .flag-section .flag-revocation{background:url(//gtms03.alicdn.com/tps/i3/TB1LEk.FpXXXXcLbVXXTJKB.XXX-121-67.png) no-repeat 0 0}
.sf-pai-item-list .pai-item .link-wrap .tag-section{position:absolute;top:0;left:0;width:100%;zoom:1;clear:none!important}
.sf-pai-item-list .pai-item .link-wrap .tag-section:after{content:"";clear:both;display:block!important;height:0}
.sf-pai-item-list .pai-item .link-wrap .tag-section .pai-tag{position:relative;float:right;margin-right:2px;width:45px;height:22px;line-height:22px;text-align:center;font-family:'\5b8b\4f53',arial tahoma,'Hiragino Sans GB',sans-serif;display:none}
.sf-pai-item-list .pai-item .link-wrap .tag-section .pai-tag em{position:absolute;width:45px;height:23px;left:0;font-style:normal}
.sf-pai-item-list .pai-item .link-wrap .tag-section .pai-tag-show{display:block}
.sf-pai-item-list .pai-item .link-wrap .tag-section .tag-buy-restrictions{background:#48b800;color:#48951b;border-top:1px solid #3a9300}
.sf-pai-item-list .pai-item .link-wrap .tag-section .tag-buy-restrictions em{top:1px;color:#e2ffc3}
.sf-pai-item-list .pai-item .link-wrap .tag-section .tag-support-loans{background:#e5d305;color:#faea2f;line-height:26px;border-top:1px solid #b7a904}
.sf-pai-item-list .pai-item .link-wrap .tag-section .tag-support-loans em{top:-1px;color:#8c6d00}
.sf-pai-item-list .field-hide{display:none!important}
.sf-pai-item-list .pai-status-doing .link-wrap .info-section .price-current .value{color:#d91615}
.sf-pai-item-list .pai-status-break-show,.sf-pai-item-list .pai-status-doing-show,.sf-pai-item-list .pai-status-done-show,.sf-pai-item-list .pai-status-failure-show,.sf-pai-item-list .pai-status-revocation-show,.sf-pai-item-list .pai-status-todo-show{display:none}
.sf-pai-item-list .pai-status-todo .pai-status-todo-show{display:block}
.sf-pai-item-list .pai-status-doing .pai-status-doing-show{display:block}
.sf-pai-item-list .pai-status-done .pai-status-done-show{display:block}
.sf-pai-item-list .pai-status-failure .pai-status-failure-show{display:block}
.sf-pai-item-list .pai-status-break .pai-status-break-show{display:block}
.sf-pai-item-list .pai-status-revocation .pai-status-revocation-show{display:block}

/* auction detail */
.bid-over{color:#070707;padding:10px 20px;height:136px}
.bid-over h1{font-size:20px;text-align:center}
.bid-over .bid-fail s{width:21px;height:21px;vertical-align:middle;margin-right:3px;position:relative;top:-1px;background:transparent url(<%= asset_url'auction_hammer.png'%>) no-repeat}
.bid-fail .bid-price{font-size:24px;color:#d91615;margin-left:4px}
.bid-fail .bid-price em.m-i{font-size:24px;font-weight:700}
.bid-fail .bid-result{font-size:14px;color:#333;font-weight:400}
.bid-over .bid-red{border:1px solid #e5bbbb;color:#d91615;height:28px;display:block;float:left;padding:0 12px;line-height:28px;margin-left:130px;margin-right:10px}
.bid-over .bid-black{color:#333;border:1px solid #c9c9c9;height:28px;line-height:28px;display:block;float:left;padding:0 12px}
.bid-over .bid-success s{width:24px;height:22px;vertical-align:middle;margin-right:3px;position:relative;top:-3px;background:transparent url(<%= asset_url 'auction_combo.png'%>) no-repeat 0 -384px}

.pm-bid{background-color:#f3f3f3;padding:12px 20px 11px;color:#666;min-height:136px}
.pm-bid s,.top-msg s{display:inline-block;background:transparent url(//gtms03.alicdn.com/tps/i3/T1RsbNXmBaXXa6GBjT-41-1166.png) no-repeat 0 0;*zoom:1;*display:inline}
.pm-bid input,.pm-bid select{border:1px solid #c2bbba;vertical-align:middle;font-size:20px;padding-left:10px}
.pm-message .pm-agency-chk{border-width:0}
.pm-bid dl{margin-top:10px;margin-top:20px\9}
.pm-bid dd,.pm-bid dt{float:left;position:relative;width:403px}
.pm-bid dt{width:50px;text-align:left;margin-right:15px;height:40px;line-height:40px}
.pm-message dt{height:28px;line-height:28px}
.pm-message .line{margin-bottom:15px;margin-top:5px}
.pm-bid .pm-num-h{margin-left:25px;margin-right:10px;width:25px}
.pm-bid .pm-price-input{width:236px;height:40px;float:left;line-height:40px}
.pm-bid .login-tip{font-size:14px;font-weight:bolder}
.pm-bid .login-tip,.pm-bid .login-tip:hover{text-decoration:underline}

.pm-subscribe{margin-left:15px;vertical-align:middle;float:right;margin-right:20px;padding-left:10px;border-left:1px solid #ccc;z-index:100}
.pm-subscribe-tip{margin-top:23px;margin-left:-15px}

.pai-pay-infor{border-collapse:collapse;width:100%;height:80px}
.pai-pay-infor tr{padding:0}
.pai-pay-infor td{font-family:tahoma;font-size:12px;color:#777;cursor:pointer}
.pay-mark{padding-left:20px;width:50px;color:#666;height:24px;line-height:20px;overflow:hidden;text-align:justify;text-align-last:justify;text-justify:distribute-all-lines;vertical-align:-10px;-moz-text-align-last:justify;-webkit-text-align-last:justify}
.pay-mark:after{display:inline-block;content:'';overflow:hidden;width:100%;height:0}
.pay-price{font-family:tahoma}
.pai-space{padding-left:6px}
.pay-first{position:relative;width:72px}
.pay-first .pay-first-icon{position:absolute;top:50%;margin-top:-8px;left:6px;width:12px;height:12px;background-position:-40px -40px}
.pay-type{color:#333}
.pay-type-help{display:inline-block;*display:inline;*zoom:1;width:13px;height:13px;background-position:0 -160px;margin-left:5px;vertical-align:-2px}
